<VirtualHost *:80>
		ServerName {{server_name}}
		ServerAdmin admin
		WSGIScriptAlias / {{wsgi_module}}
		<Directory {{move_directory}}>
			Order allow,deny
			Allow from all
		</Directory>
		Alias /static {{static_path}}
		<Directory {{static_path}}>
			Order allow,deny
			Allow from all
		</Directory>
		ErrorLog ${APACHE_LOG_DIR}/error.log
		LogLevel warn
		CustomLog ${APACHE_LOG_DIR}/access.log combined
</VirtualHost>
